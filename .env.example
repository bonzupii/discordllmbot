# Discord
DISCORD_TOKEN=
DISCORD_CLIENT_ID=

# LLM API Keys (can now be managed in dashboard)
GEMINI_API_KEY=
OLLAMA_API_KEY=
QWEN_API_KEY=

## Optional Qwen OAuth overrides (dashboard works without these using public PKCE client defaults)
QWEN_OAUTH_CLIENT_ID=
QWEN_OAUTH_CLIENT_SECRET=
QWEN_OAUTH_AUTH_URL=https://chat.qwen.ai/oauth/authorize
QWEN_OAUTH_TOKEN_URL=https://chat.qwen.ai/oauth/token
# If empty, API will auto-derive from request host as /api/llm/qwen/oauth/callback
QWEN_OAUTH_REDIRECT_URI=
QWEN_OAUTH_SCOPE=openid profile

# PostgreSQL
POSTGRES_DB=
POSTGRES_USER=
POSTGRES_PASSWORD=
DATABASE_URL=
POSTGRES_PORT=

# pgAdmin
PGADMIN_DEFAULT_EMAIL=
PGADMIN_DEFAULT_PASSWORD=

# API
API_PORT=3000

# OLLAMA
OLLAMA_API_URL=http://host.docker.internal:11434

# Qwen OpenAI-compatible endpoint
QWEN_API_URL=https://dashscope-intl.aliyuncs.com/compatible-mode/v1

# Dashboard
DASHBOARD_PORT=5173
DOCS_PORT=5174
